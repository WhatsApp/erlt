-module(count).

-export([main/1]).

main(_Args) ->
    count_server:start(numbers, 10),
    true = count_server:equal(numbers, 10),
    count_server:inc(numbers, 5),
    false = count_server:equal(numbers, 10),
    true = count_server:equal(numbers, 15),
    count_server:dec(numbers, 3),
    false = count_server:equal(numbers, 15),
    true = count_server:equal(numbers, 12),
    11 = count_server:closer(numbers, 11, 17),
    count_server:stop(numbers),
    io:format("[ok]~n", []).
