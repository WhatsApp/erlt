-module(lists).
-export([filter/2, seq/2, seq/3, zip/2]).

-spec filter(fun((T) -> boolean()), [T]) -> [T].
filter(_Fun, _L) ->
    erlang:error("stub"),
    [].

-spec seq(integer(), integer()) -> [integer()].
seq(_, _) ->
    erlang:error("stub"),
    [].

-spec seq(integer(), integer(), integer()) -> [integer()].
seq(_, _, _) ->
    erlang:error("stub"),
    [].

-spec zip([A], [B]) -> [{A, B}].
zip(_, _) ->
    erlang:error("stub"),
    [].
